<script type="text/javascript">
  function initializeMap() {
    console.log("initializ map");
    var latLng = new google.maps.LatLng(47.576641, -122.168734);
    var mapOptions = {
      center: latLng,
      zoom: 14
    };
    var map = new google.maps.Map(document.getElementById('map-factoria'),
        mapOptions);

    var marker = new google.maps.Marker({
        position: latLng,
        map: map,
        title: 'Regent'
    });
  }
  google.maps.event.addDomListener(window, 'turbolinks:load', initializeMap);
</script>

